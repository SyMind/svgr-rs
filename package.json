{
  "name": "@svgr-rs/core",
  "version": "0.0.1",
  "main": "index.js",
  "types": "index.d.ts",
  "author": "SyMind <dacongsama@live.com>",
  "napi": {
    "name": "svgr-rs",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-pc-windows-msvc",
        "x86_64-apple-darwin",
        "x86_64-pc-windows-msvc",
        "universal-apple-darwin"
      ]
    }
  },
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.15.2",
    "@types/node": "^18.15.11",
    "ava": "^5.1.1"
  },
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --platform --release --js binding.js --dts binding.d.ts",
    "build:debug": "napi build --platform --js binding.js --dts binding.d.ts",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "ava",
    "universal": "napi universal",
    "version": "napi version"
  },
  "packageManager": "yarn@3.5.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "keywords": [
    "svgr",
    "svg",
    "react",
    "core",
    "api",
    "swc"
  ],
  "homepage": "https://github.com/SyMind/svgr-rs",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SyMind/svgr-rs.git"
